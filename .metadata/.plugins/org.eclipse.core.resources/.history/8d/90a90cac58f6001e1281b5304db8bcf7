package TestNG;

import org.testng.annotations.AfterClass;
import org.testng.annotations.AfterMethod;
import org.testng.annotations.AfterSuite;
import org.testng.annotations.AfterTest;
import org.testng.annotations.BeforeClass;
import org.testng.annotations.BeforeMethod;
import org.testng.annotations.BeforeSuite;
import org.testng.annotations.BeforeTest;
import org.testng.annotations.Test;

public class TestNG1 {
	@Test
	public void Test1()
	{
		System.out.println("I am in a test1 method");
	}
	@BeforeTest
	public void b()
	{
		System.out.println("I am in BeforeTest");
	}
	@BeforeSuite
	public void c()
	{
		System.out.println("I am in BeforeSuite");
	}

	@AfterMethod
	public void d()
	{
		System.out.println("I am in AfterMethod");
	}
	@BeforeClass
	public void e()
	{
		System.out.println("I am in BeforeClass");
	}
	@AfterTest
	public void f()
	{
		System.out.println("I am in AfterTest");
	}
	@AfterClass
	public void g()
	{
		System.out.println("I am in AfterClass");
	}
	@BeforeMethod
	public void h()
	{
		System.out.println("I am in BeforeMethod");
	}
	@AfterSuite
	public void i()
	{
		System.out.println("I am in AfterSuite");
	}
	@Test
	public void Test2()
	{
		System.out.println("I am in a test2 method");
	}
	}


