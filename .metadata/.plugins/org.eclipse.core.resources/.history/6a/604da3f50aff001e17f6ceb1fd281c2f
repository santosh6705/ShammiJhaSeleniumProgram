package JavaPackage;

public class Variable1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a=5;
		System.out.println(a);
		
		int x=100;
		System.out.println(x);
		
		//modified x value
		 x=200;
		 System.out.println(x);
		 
		 char c='a';
		 System.out.println(c);

	}

}
