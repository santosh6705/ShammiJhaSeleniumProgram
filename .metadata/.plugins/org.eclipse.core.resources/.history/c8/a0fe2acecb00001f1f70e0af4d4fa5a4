package InterviewJavaProgram;

public class MaximumNoArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a[]= {12,34,56,77};
		for(int i=0;i>=a.length;i++)
		{
			if(a[i]>a[i+1])
			{
				System.out.println("The maximum number is "+a[i]);
			}
		}

	}

}
