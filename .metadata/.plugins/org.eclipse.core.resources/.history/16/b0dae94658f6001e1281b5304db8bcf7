package TestNG;

import org.testng.annotations.AfterClass;
import org.testng.annotations.AfterMethod;
import org.testng.annotations.AfterSuite;
import org.testng.annotations.AfterTest;
import org.testng.annotations.BeforeClass;
import org.testng.annotations.BeforeMethod;
import org.testng.annotations.BeforeSuite;
import org.testng.annotations.BeforeTest;
import org.testng.annotations.Test;

public class TestNG1 {
	@Test
	public void a1()
	{
		System.out.println("I am in a test method");
	}
	@BeforeTest
	public void b()
	{
		System.out.println("I am in b test method");
	}
	@BeforeSuite
	public void c()
	{
		System.out.println("I am in c test method");
	}

	@AfterMethod
	public void d()
	{
		System.out.println("I am in d test method");
	}
	@BeforeClass
	public void e()
	{
		System.out.println("I am in e test method");
	}
	@AfterTest
	public void f()
	{
		System.out.println("I am in f test method");
	}
	@AfterClass
	public void g()
	{
		System.out.println("I am in g test method");
	}
	@BeforeMethod
	public void h()
	{
		System.out.println("I am in h test method");
	}
	@AfterSuite
	public void i()
	{
		System.out.println("I am in i test method");
	}
	@Test
	public void a2()
	{
		System.out.println("I am in a test method");
	}
	}


